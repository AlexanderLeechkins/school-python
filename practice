
list=[]
s=0


def removeoflist():
    popnumber=int(input("what number of the list would you like removed(N for no more) "))
    list.pop(popnumber)
    s=-1
        
def addtolist():
    addnumber=int(input("what number would you like to add to the list "))
    list.append(addnumber)
    s=+1

def peeklist():
    print(list[s])

print("pop\n push\n peek\n")
userinput=input("what would you like to do to the list ")
while userinput =="pop" or "push" or "peek":
    if userinput== "pop":
        removeoflist()
    if userinput =="push":
        addtolist()
    if userinput =="peek":
        peeklist()


